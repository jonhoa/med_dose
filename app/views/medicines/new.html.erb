<h1>Create your Medicine Prescription </h1>
<%= form_with url:"/medicines/new", method: :post do |form|%>
<div><hr/>
<%=form.label :name%>
<%=form.text_field :name%>
</div>
<div>
<%=form.label :dosage%>
<%=form.number_field :dosage%>
</div>
<div>
<%=form.label :frequency%>
<%=form.number_field :frequency%>
</div>
<%=form.submit "Save", class: "btn btn-primary"%>
<%end%>

<%= link_to "Back", :back%>